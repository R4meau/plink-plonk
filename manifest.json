{
  "manifest_version": 2,
  "name": "Plink Plonk",
  "description": "A minimal sound debugger for web pages",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}